@model ParcVehicule.ViewModels.VehiculeVM

<h2>Tous les véhicules</h2>

<table class="rounded-t-lg m-5 w-5/6 mx-auto bg-gray-800 text-gray-200">
    <tr class="text-left border-b border-gray-300">
        <th class="px-4 py-3">Marque</th>
        <th class="px-4 py-3">Modèle</th>
        <th class="px-4 py-3">Puissance</th>
        <th class="px-4 py-3">Type</th>
    </tr>
    @foreach (var vehicule in Model.ListeVehicule)
    {
        <tr class="bg-gray-700 border-b border-gray-600">
            <td class="px-4 py-3">@vehicule.Marque</td>
            <td class="px-4 py-3">@vehicule.Modele</td>
            <td class="px-4 py-3">@vehicule.Puissance</td>
            <td class="px-4 py-3">@vehicule.type</td>
        </tr>
    }
    </table>
